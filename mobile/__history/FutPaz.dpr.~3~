program FutPaz;

uses
  System.StartUpCopy,
  FMX.Forms,
  UPrincipal in 'UPrincipal.pas' {FrmPrincipal},
  UJogadores in 'UJogadores.pas' {FrmJogador},
  UCadAtleta in 'UCadAtleta.pas' {FrmCadAtleta};

