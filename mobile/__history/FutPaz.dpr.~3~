begin
  Application.Initialize;
  Application.CreateForm(TFrmPrincipal, FrmPrincipal);
  Application.CreateForm(TFrmJogador, FrmJogador);
  Application.CreateForm(TFrmCadAtleta, FrmCadAtleta);
  Application.Run;
